{
    "name": "nazka/location-bundle",
    "type": "symfony-bundle",
    "description": "Addresses, countries and provinces doctrine entities.",
    "license": "GPLv2",
    "authors": [
        {
            "name": "Javier Sampedro",
            "email": "jsampedro77@gmail.com",
            "homepage": "http://sampedrobcn.es"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.3.3",

        "symfony/framework-bundle":        "~2.3",
        "stof/doctrine-extensions-bundle": "1.1.*",
        "friendsofsymfony/jsrouting-bundle": "@stable",
        "jms/serializer-bundle": "dev-master"
    },
    "require-dev": {
        "doctrine/orm":      "~2.2",
        "symfony/form":      "~2.3",
        "symfony/validator": "~2.3"
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-0": { "Nazka\\LocationBundle": "" }
    },
    "target-dir": "Nazka/LocationBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "0.2.x-dev"
        }
    }
}
